                            <pertinentFinding classCode="OBS" moodCode="EVN">
                                <id root="{{findingId}}"></id>
                            </pertinentFinding>
                        </pertinentInformation1>
                    {{/findingId}}
                {{/supportingInformation}}
                {{#author}}
                    {{> Author.mustache}}
                {{/author}}
                {{#informant}}
